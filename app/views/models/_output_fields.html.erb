<div class="nested-fields">
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control", :placeholder => "name of what you are predicting" %>
  </div>

  <div class="form-group">
    <%= f.label :kind %>
    <%= f.text_field :kind, class: "form-control", :placeholder => "text or number" %>
  </div>

  <div class="form-group">
    <%= f.label :order %>
    <%= f.text_area :order, class: "form-control", :placeholder => "number" %>
  </div>

  <div class="form-group">
    <%= f.label :default_value %>
    <%= f.text_field :default_value, class: "form-control", :placeholder => "just put 0 here" %>
  </div>

  <%= link_to_remove_association "remove output", f %>
</div>