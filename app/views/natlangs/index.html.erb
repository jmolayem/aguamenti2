<p id="notice"><%= notice %></p>
<div class="center">
<%= link_to new_natlang_path, class: "btn btn-primary", data: { no_turbolink: true } do %>
  <i class="glyphicon glyphicon-plus"></i> New Brain
<% end %>
</div>

<div class="center">
<div class="row">
  <% @natlangs.each do |natlang| %>
   <div class="col-md-3 col-sm-4 col-xs-12">
    <div class="panel panel-primary text-center">
      <div class="panel-body">
        <%= image_tag natlang.image.url(:medium) %>
        <div class="panel-footer">
        <h4><%= natlang.name %>
        <span class="label label-default"><%= convnetimage.size %></span>
        <span class="label label-success"><%= convnetimage.accuracy %></span></h4>
          <%= link_to 'Use', natlang, class: "btn btn-primary"%>
          <% if user_signed_in? %>
            <% if current_user.admin? %>
          <%= link_to 'Edit', edit_natlang_path(natlang), class: "glyphicon glyphicon-pencil" %>
          <%= link_to 'Destroy', natlang, method: :delete, data: { confirm: 'Are you sure?' }, class: "glyphicon glyphicon-trash" %>
            <% end %>
          <% end %>
        </div>
      </div>
    </div>
    </div>
  <% end %>
    </div>
    </div>
<br>
<%= will_paginate @models, renderer: BootstrapPagination::Rails  %>
<br>
</br>
</br>
</br>
