<aside class="bg-dark">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2 text-center">
              <br>
<br>
<br>
                <h2 class="section-heading">Upcoming this Week</h2>
                <hr class="light">
      <table class="table table-striped table-bordered table-hover">
  <thead>
<tr>
    <th class="center">Release</th>
    <th class="center">Name</th>
    <th class="center">Organization</th>
    <th class="center">City</th>
    <th class="center">State</th>
    <th class="center">Source</th>
    <th class="center">Type</th>
</tr>

  <% @upcomings.each do |upcoming| %>
<tr>
            <td><%= upcoming.newdate %></td>
            <td><%= upcoming.name %></td>
            <td><%= upcoming.organization %></td>
            <td><%= upcoming.city %></td>
            <td><%= upcoming.state %></td>
            <td><%= upcoming.sauce %></td>
            <td><%= upcoming.modeltyp %></td>
            </tr>
            <% end %>
          </table>
<br>

<div class="container">
<%= will_paginate @upcoming, renderer: BootstrapPagination::Rails  %>
</div>

<% if user_signed_in? %>
<% if current_user.admin? %>
<div class="center">
<%= link_to new_upcoming_path, class: "btn btn-primary", data: { no_turbolink: true } do %>
  <i class="glyphicon glyphicon-plus"></i> New Dataset
<% end %>
</div>
<% end %>
<% end %>
            </div>
        </div>
    </div>
</aside>
